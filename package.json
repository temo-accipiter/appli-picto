{
  "name": "dashboard",
  "description": "dashboard personnalisé",
  "author": "Miminoshvili Temo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:fast": "next dev --turbopack & sleep 30 && node scripts/precompile-routes.cjs",
    "build": "next build",
    "build:prod": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "preview": "next start",
    "check": "pnpm lint:fix && pnpm format",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "verify:all": "pnpm verify:ci && pnpm test:e2e",
    "verify:pre-commit": "pnpm type-check && pnpm lint && pnpm test",
    "verify": "pnpm type-check && pnpm lint && pnpm format:check && pnpm test && pnpm build:prod",
    "verify:quick": "pnpm type-check && pnpm lint && pnpm build",
    "verify:ci": "pnpm type-check && pnpm lint && pnpm format:check && pnpm test:coverage && pnpm build:prod",
    "check-bundle": "node scripts/check-bundle-size.js",
    "db:link": "dotenv -e .env -- sh -lc 'supabase link --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "db:dump": "supabase db dump --schema public,storage -f supabase/schema.sql --local",
    "db:types": "mkdir -p src/types && supabase gen types typescript --local > src/types/supabase.ts",
    "context:update": "pnpm db:dump && pnpm db:types",
    "db:dump:remote": "dotenv -e supabase/.env -- sh -lc 'PGPASSWORD=\"$SUPABASE_DB_PASSWORD\" PGSSLMODE=require pg_dump -h \"$SUPABASE_DB_HOST\" -p \"$SUPABASE_DB_PORT\" -U \"$SUPABASE_DB_USER\" -d \"$SUPABASE_DB_NAME\" --schema-only --schema=public --schema=storage -f supabase/schema.sql'",
    "db:types:remote": "dotenv -e supabase/.env -- sh -lc 'mkdir -p src/types && supabase gen types typescript --project-id \"$SUPABASE_PROJECT_REF\" > src/types/supabase.ts'",
    "context:update:remote": "pnpm db:dump:remote && pnpm db:types:remote",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:serve": "dotenv -e .env -- sh -lc 'supabase functions serve --env-file ./supabase/.env.local'",
    "deploy:checkout": "dotenv -e .env -- sh -lc 'supabase functions deploy create-checkout-session --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "deploy:webhook": "dotenv -e .env -- sh -lc 'supabase functions deploy stripe-webhook --no-verify-jwt --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "logs:checkout": "dotenv -e .env -- sh -lc 'supabase functions logs create-checkout-session --project-ref \"$SUPABASE_PROJECT_REF\" --follow'",
    "logs:webhook": "dotenv -e .env -- sh -lc 'supabase functions logs stripe-webhook --project-ref \"$SUPABASE_PROJECT_REF\" --follow'",
    "stripe:listen": "stripe listen --forward-to localhost:54321/functions/v1/stripe-webhook",
    "stripe:trigger:checkout": "stripe trigger checkout.session.completed",
    "stripe:trigger:subscription": "stripe trigger customer.subscription.created",
    "stats": "cloc src/ --exclude-dir=test,tests --by-file-by-lang",
    "stats:summary": "cloc src/ --exclude-dir=test,tests",
    "clean": "rm -rf node_modules .next coverage",
    "clean:all": "pnpm clean && rm -rf pnpm-lock.yaml",
    "postinstall": "pnpm db:types || true",
    "debug:verify": "pnpm type-check && echo '✅ TypeScript passed' && pnpm lint && echo '✅ ESLint passed' && pnpm format:check && echo '✅ Prettier passed' && pnpm test:coverage && echo '✅ Tests passed' && pnpm build:prod && echo '✅ Build passed' && pnpm test:e2e && echo '✅ E2E passed'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@sentry/nextjs": "^10.25.0",
    "@stripe/stripe-js": "^8.4.0",
    "@supabase/supabase-js": "^2.81.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.10.1",
    "heic2any": "^0.0.4",
    "i18next": "^25.0.0",
    "i18next-http-backend": "^3.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.553.0",
    "marked": "^17.0.0",
    "next": "16.0.3",
    "react": "^19.0.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.3.3",
    "react-turnstile": "^1.1.4",
    "react-use": "^17.6.0",
    "sass": "^1.86.3",
    "stripe": "^19.3.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@eslint/js": "^9.24.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/deno": "^2.5.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "axe-core": "^4.11.0",
    "baseline-browser-mapping": "^2.9.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "20.19.4"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": "1.18.1"
    }
  },
  "resolutions": {
    "@modelcontextprotocol/sdk": "1.18.1"
  }
}
