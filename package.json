{
  "name": "dashboard",
  "description": "dashboard personalisÃ©",
  "author": "Miminoshvili Temo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "yarn lint:fix && yarn format",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write .",
    "db:link": "dotenv -e .env -- sh -lc 'supabase link --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "db:dump": "dotenv -e .env -- sh -lc 'PGPASSWORD=\"$SUPABASE_DB_PASSWORD\" PGSSLMODE=require pg_dump -h \"$SUPABASE_DB_HOST\" -p \"$SUPABASE_DB_PORT\" -U \"$SUPABASE_DB_USER\" -d \"$SUPABASE_DB_NAME\" --schema-only --schema=public --schema=storage -f supabase/schema.sql'",
    "db:types": "dotenv -e .env -- sh -lc 'mkdir -p src/types && supabase gen types typescript --project-id \"$SUPABASE_PROJECT_REF\" > src/types/supabase.ts'",
    "context:update": "yarn db:dump && yarn db:types",
    "supabase:serve": "dotenv -e .env -- sh -lc 'supabase functions serve --env-file ./supabase/.env.local'",
    "deploy:checkout": "dotenv -e .env -- sh -lc 'supabase functions deploy create-checkout-session --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "deploy:webhook": "dotenv -e .env -- sh -lc 'supabase functions deploy stripe-webhook --no-verify-jwt --project-ref \"$SUPABASE_PROJECT_REF\"'",
    "logs:checkout": "dotenv -e .env -- sh -lc 'supabase functions logs create-checkout-session --project-ref \"$SUPABASE_PROJECT_REF\" --follow'",
    "logs:webhook": "dotenv -e .env -- sh -lc 'supabase functions logs stripe-webhook --project-ref \"$SUPABASE_PROJECT_REF\" --follow'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.50.3",
    "framer-motion": "^12.10.1",
    "i18next": "^25.0.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.501.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.5.0",
    "react-turnstile": "^1.1.4",
    "react-use": "^17.6.0",
    "sass": "^1.86.3",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/stripe": "^8.0.417",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "vite": "^6.3.0"
  },
  "volta": {
    "node": "20.19.4"
  }
}
